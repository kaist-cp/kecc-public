
fun i32 @foo () {
init:
  bid: b0
  allocations: 
    %l0:i32:sum
    %l1:i32:i

block b0:
  %b0:i0:unit = nop
  %b0:i1:unit = nop
  j b2(0:i32, 0:i32)

block b2:
  %b2:p0:i32:sum
  %b2:p1:i32:i
  %b2:i0:unit = nop
  %b2:i1:u1 = cmp eq %b2:p1:i32 5:i32
  br %b2:i1:u1, b6(), b7()

block b6:
