  ret %b2:p0:i32

block b7:
  %b7:i0:unit = nop
  %b7:i1:u1 = cmp eq %b2:p1:i32 3:i32
  br %b7:i1:u1, b10(), b11()

block b10:
  %b10:i0:unit = nop
  %b10:i1:i32 = add %b2:p1:i32 1:i32
  %b10:i2:unit = nop
  j b2(%b2:p0:i32, %b10:i1:i32)

block b11:
  %b11:i0:unit = nop
  %b11:i1:unit = nop
  %b11:i2:i32 = add %b2:p0:i32 %b2:p1:i32
  %b11:i3:unit = nop
  %b11:i4:unit = nop
  %b11:i5:i32 = add %b2:p1:i32 1:i32
  %b11:i6:unit = nop
  j b2(%b11:i2:i32, %b11:i5:i32)
